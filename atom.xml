<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Siyuan Zhang</title>
 <link href="http://zedis.me/atom.xml" rel="self"/>
 <link href="http://zedis.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://zedis.me/</id>
 <author>
   <name>Siyuan Zhang</name>
   <email>kastnerorz@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://zedis.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://zedis.me{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
